  "plugins": [
    "language-picker",
    "-sharing",
    "sharing-plus"
  ],
  "pluginsConfig": {
    "language-picker": {
      "grid-columns": 2
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": true,
      "instapaper": false,
      "vk": false,
      "all": [
